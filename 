[32m+[m[32musing System.Collections;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class Player : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    // Start is called before the first frame update[m
[32m+[m[32m    private float _speed = 3.5f;[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m        //take the current position = new position (0, 0, 0)[m
[32m+[m[32m        transform.position = new Vector3(0, 0, 0);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update is called once per frame[m
[32m+[m[32m    void Update()[m
[32m+[m[32m    {[m
[32m+[m[32m        //new Vector3(1, 0, 0); < same thing as Vector3.right[m
[32m+[m[32m        transform.Translate(Vector3.right * _speed * Time.deltaTime);[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[32m}[m
